                    <label for="status">Status:</label>
                    <input type="text" id="status" name="status" placeholder="Buscar" class="form-control">
                </div>
                <div class="filter-group">
                    <label for="tipo-transporte">Tipo transporte:</label>
                    <input type="text" id="tipo-transporte" name="tipo-transporte" placeholder="Buscar" class="form-control">
                </div>
                <div class="filter-group">
                    <label for="desde">Desde:</label>
